<?xml version="1.0" ?>
<yard name="Solidus">

	<property id="name" value="cention-solidus" />
	<property id="version" value="3.2" />
	
	<collection id="solidus">
		<property type="prefix" value="$(FeriteModuleSourceDir)" />

		<list type="source">
			<file name="ferite-module-bright.feh" />
		</list>
	</collection>

	<phase id="install">
		<perform action="install" target="bright" />
	</phase>
</yard>
