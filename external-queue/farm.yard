<?xml version="1.0" ?>
<yard name="ExternalQueue">

	<property id="name" value="cention-external-queue" />
	<property id="version" value="3.2" />
	
	<collection id="external-queue">
		<property type="prefix" value="$(FeriteModuleSourceDir)" />

		<list type="source">
			<file name="ExternalQueue.feh" />
			<file name="ClearIt.feh" />
		</list>
	</collection>

	<phase id="install">
		<perform action="install" target="external-queue" />
	</phase>
</yard>
