<?xml version="1.0" ?>
<yard name="logger">

	<property id="name" value="cention-logger" />
	<property id="version" value="3.7" />
	
	<collection id="logger">
		<property type="prefix" value="$(FeriteModuleSourceDir)" />  

		<list type="source">
			<file name="logger.feh" />
		</list>
	</collection>

	<phase id="install">
		<perform action="install" target="logger" />
	</phase>
</yard>
