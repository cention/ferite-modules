<?xml version="1.0" ?>
<yard name="twitter">

	<property id="name" value="cention-twitter" />
	<property id="version" value="3.2" />

	<module id="twitter">
		<list type="source">
			<file name="twitter.feh" />
		</list>
		<property type="prefix" value="$(FeriteModuleNativeDir)" />        
	</module>

	<phase id="install">
		<perform action="install" target="twitter" />
		<copy file="twitter.feh" target="$(FeriteModuleSourceDir)" />
	</phase>

</yard>

